{
  "name": "qumare-syntax",
  "displayName": "QuMaRE Syntax",
  "description": "TextMate grammar and snippets for QuMaRE experiment files.",
  "version": "0.1.0",
  "publisher": "quiqcl",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "qumare",
        "aliases": [
          "QuMaRE",
          "qumare"
        ],
        "extensions": [
          ".qmr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "qumare",
        "scopeName": "source.qumare",
        "path": "./syntaxes/qumare.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "qumare",
        "path": "./snippets/qumare.code-snippets"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quiqcl/qumare.git"
  }
}
